<script>
setInterval(f,100);

var x = 900 ;
var y = 1 ;

function f(){
    x -= 5;
    if(y<31){
        y += 1;
    }else{
        y = 1;
    }
    zombie.style.left = x;
    zombie_pic.src="1 ("+y+").png"
};

</script>
